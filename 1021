#include<stdio.h>
int main()
{
    int i , j ;
    float n;
    float notes[12]={100.00,50.00,20.00,10.00,5.00,2.00,1.00,0.50,0.25,0.10,0.05,0.01};
    float stores[12];

    scanf("%f",&n);

    for(i=0 ; i<12 ; i++){
        stores[i]=n/notes[i];
        n=n%notes[i];
    }

    for(j=0 ; j<12 ; j++){
        printf("%.f nota(s) de R$ %.2f\n",store[j],note[j]")
    }

    return 0;
}

