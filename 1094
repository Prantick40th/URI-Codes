#include<stdio.h>
int main()
{
    int i,n,x;
    long int t_c=0,t_r=0,t_s=0,sum=0;
    char ch;
    char z = '%';
    float c,r,s;

    scanf("%d",&n);

    for(i=1 ; i<=n ; i++){
        scanf("%d %c",&x,&ch);

        sum = sum + x;

        if(ch=='C')
            t_c+=x;
        else if(ch=='R')
            t_r+=x;
        else if(ch=='S')
            t_s+=x;
    }
    c=(float)t_c/(float)sum*100.00;
    r=(float)t_r/(float)sum*100.00;
    s=(float)t_s/(float)sum*100.00;

    printf("Total: %d cobaias\n",sum);
    printf("Total de coelhos: %d\n",t_c);
    printf("Total de ratos: %d\n",t_r);
    printf("Total de sapos: %d\n",t_s);
    printf("Percentual de coelhos: %.2f %c\n",c,z);
    printf("Percentual de ratos: %.2f %c\n",r,z);
    printf("Percentual de sapos: %.2f %c\n",s,z);
    
    return 0;
}
