#include<stdio.h>
int main()
{
    int a,b,x,y;
    scanf("%d %d",&a,&b);

    if(a>b){
        x = (24 - a + b);
        printf("O JOGO DUROU %d HORA(S)\n",x);
    }
    else if(a<b){
        y = (b-a);
        printf("O JOGO DUROU %d HORA(S)\n",y);
    }
    else if(a == 0 && b == 0)
        printf("O JOGO DUROU %d HORA(S)\n",24);

return 0;
}

