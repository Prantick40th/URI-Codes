#include<stdio.h>
int main()
{
    float a,b,c,d,e;
    double a1,a2;

    scanf("%f%f%f%f",&a,&b,&c,&d);

    a1 = ((a*2)+(b*3)+(c*4)+(d*1))/10;

    printf("Media: %.1lf\n",a1);

    if(a1 >= 7.0)
        printf("Aluno aprovado\n");
    else if(a1 < 5)
        printf("Aluno reprovado\n");
    else
    {
        printf("Aluno em exame\n");
        scanf("%f",&e);

        a2 = (a1+e)/2;

        if(a2 >= 5.00)
            printf("Aluno aprovado\n");
        else if(a2 <= 4.9)
            printf("Aluno reprovado\n");

        printf("Media final: %.1lf\n",a2);

    }

    return 0;
}
